 <div class='nested-fields'>
  <div class="field">
    <%= f.label :room_id, t("room.item"), class:"label"  %>
    <%= f.collection_select :room_id, Room.all,
      :id, :room_type, {class: "form-control input-add-new"} %>
  </div>
  <div class="field">
    <%= f.label :price, t("price"), class:"label"  %>
    <%= f.number_field :price, min: 0, value: 0, class: "form-control input-add-new" %>
  </div>
  <div style="margin-left:180px;">
  # <%= link_to_remove_association t("delete"), f %>
  </div>
</div>
